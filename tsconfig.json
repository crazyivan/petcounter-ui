{
  "compilerOptions": {
    "lib": ["es6", "dom", "esnext"],
    "allowJS": true,
    "allowSyntheticDefaultImports": true
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "target": "es6",
    "module": "es6",
    "jsx": "react",
    "noImplicitAny": true,
  },
  "include": ["src"],
  "exclude": ["node_modules"],
}
